@model Vehicle

<div class="header">
    <h1>Recently Added Vehicles For Sale</h1>
</div>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Year</th>
            <th scope="col">Make</th>
            <th scope="col">Model</th>
            <th scope="col">Color</th>
            <th scope="col">Mileage</th>
            <th scope="col">Vehicle Type</th>
            <th scope="col">Engine Type</th>
            <th scope="col"># of Seats</th>
            <th scope="col">Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var v in ViewBag.AllVehicles)
        {
            <tr>
                <td>@v.Year</td>
                <td>@v.Make</td>
                <td>@v.Model</td>
                <td>@v.Color</td>
                <td>@v.Mileage</td>
                <td>@v.VehicleType</td>
                <td>@v.EngineType</td>
                <td>@v.NumberOfSeats</td>
                <td>@v.Price</td>
            </tr>
        }
    </tbody>
</table>